<body class="no-skin">
    <div id="div_loading" style="display:none">
        <div class="loading-content">
            <div class="lds-spinner"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div>
            <p>Đang xử lý ....</p>
        </div>
    </div>
    <script>
        function ShowAjaxLoading() {
            $("#div_loading").show();
        }
        function HideAjaxLoading() {
            $("#div_loading").hide();
        }
        $(document).ready(function () {
            $(document).bind("ajaxSend", function () {
                ShowAjaxLoading();
            }).bind("ajaxSuccess", function () {
                HideAjaxLoading();
            }).bind("ajaxError", function () {
                HideAjaxLoading();
            }).bind("ajaxComplete", function () {
                HideAjaxLoading();
            });
        });
    </script>
    <div class="main-container" id="main-container">
        <script type="text/javascript">
            try { ace.settings.check('main-container', 'fixed') } catch (e) { }
        </script>
        <div class="main-content">
            <div class="main-content-inner">
                <div class="page-content">
                    <div class="indicator-wrapper" style="display: none;" id="loader">

                    </div>
                    <!-- /.page-header -->
                    <div class="row">
                        <script type="text/javascript">
                            $(document).ready(function (event) {
                                $.widget("ui.dialog", $.extend({}, $.ui.dialog.prototype, {
                                    _title: function (title) {
                                        var $title = this.options.title || '&nbsp;'
                                        if (("title_html" in this.options) && this.options.title_html == true)
                                            title.html($title);
                                        else title.text($title);
                                    }
                                }));
                            });
                        </script>
                        @RenderBody()
                        <div id="modal_orther" class="modal fade" role="dialog" data-backdrop="static"></div>
                        <div id="modal_danhmuc_orther" class="modal fade" role="dialog" data-backdrop="static"></div>
                    </div><!-- /.row -->
                </div><!-- /.page-content -->
            </div>

        </div><!-- /.main-content -->

        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
            <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
        </a>
    </div><!-- /.main-container -->
    <div id="pnl-chat">
    </div>
    <div id="pnl-notification">
    </div>

    <div id="inbox-content" class="hide">

    </div><!--NỘI DUNG EMAIL-->
